{% extends 'base.html.twig' %}

{% block title %}Hello CategoryController!{% endblock %}

{% block body %}
<div class="container">
    <ul>
        {% for category in categories %}
            <li><a href="{{ path('category_show', {categoryName: category.name}) }}">{{ category.name }}</li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
